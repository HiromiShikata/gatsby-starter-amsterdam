{
  "name": "gatsby-starter-amsterdam",
  "version": "2.0.0",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "yarn run lint:css && yarn run lint:js && yarn run check-pretty",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "gatsby": "^4.1.4",
    "gatsby-plugin-manifest": "^4.1.3",
    "gatsby-theme-amsterdam": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "eslint": "8.2.0",
    "eslint-config-gatsby-standard": "^3.0.2",
    "prettier": "^2.4.1",
    "stylelint": "14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
